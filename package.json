{
  "name": "chessbot",
  "packageManager": "yarn@4.12.0",
  "type": "module",
  "scripts": {
    "start": "tsx src/main.ts",
    "lint": "eslint",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "migrate": "env-cmd node-pg-migrate",
    "generate:types": "kysely-codegen --out-file src/postgres_types.ts"
  },
  "dependencies": {
    "@slack/bolt": "^4.6.0",
    "dotenv": "^17.2.3",
    "joist-orm": "^1.282.1",
    "knex": "^3.1.0",
    "kysely": "^0.28.8",
    "peberminta": "^0.10.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@slack/types": "^2.19.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.8.1",
    "@types/pg": "^8",
    "env-cmd": "^11.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "kysely-codegen": "^0.19.0",
    "node-pg-migrate": "^8.0.3",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  }
}
